{
  "extends": ["cheesecakelabs"],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.extra.js"
      }
    }
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/stories.js",
          "**/*.dev.js",
          "**/*.test.js",
          "server.js",
          "postcss.config.js",
          "webpack.config.*",
          "storybook/*"
        ]
      }
    ]
  },
  "globals": {}
}
